<Window
   x:Class="SwainStrainTools.UI.Form_Settings"
   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
   xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
   xmlns:local="clr-namespace:SwainStrainTools.UI"
   mc:Ignorable="d"
   d:DesignHeight="300"
   d:DesignWidth="300">
   <Grid>
      <TextBox Name="License" Height="30" Width="250">
         
      </TextBox>

      <Button
         x:Name='OKBtn'
         Content='OK'
         IsDefault='True'
         Margin='0,0,95,10'
         Click='OKBtn_Click'
         Height='25'
         VerticalAlignment='Bottom'
         HorizontalAlignment="Right"
         Width="75" />
      <Button
         x:Name='CancelBtn'
         Content='Cancel'
         IsCancel='True'
         Height='25'
         Margin='0,0,10,10'
         VerticalAlignment='Bottom'
         HorizontalAlignment="Right"
         Width="75"
         Click="CancelBtn_Click" />
      <TextBox
         x:Name="License_ID"
         Height="30"
         Width="250"
         Margin="22,175,21.6,65.4" />

   </Grid>
</Window>
